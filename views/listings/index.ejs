<% layout("/layout/boilerplate")%>
<body>
    <h3>All Listings</h3>
<div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1">
 <%for(let list of lists){%>
  <a href="/listings/<%=list._id%>" class="card-link">
<div class="card cols" >
  <img src="<%=list.image%>" class="card-img-top" alt="image"style="height: 20rem;">
  <div class="card-body">
    <h5 class="card-title"><b><%=list.title%></b></h5>
    <p class="card-text">&#x20B9;<%=list.price.toLocaleString("en-IN")%>/Night</p>
  </div>
</div>
</a>
<%}%> 
</div>
</body>




